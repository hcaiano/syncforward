{
  "kind": "collectionType",
  "collectionName": "podcasts",
  "info": {
    "singularName": "podcast",
    "pluralName": "podcasts",
    "displayName": "Podcast"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "episode": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "artwork": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "description": {
      "type": "richtext"
    },
    "links": {
      "type": "component",
      "repeatable": false,
      "component": "links.track-link"
    },
    "genres": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::genre.genre",
      "mappedBy": "podcast"
    },
    "artists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::artist.artist",
      "inversedBy": "podcasts"
    }
  }
}
